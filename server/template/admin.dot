<!doctype html>
<html lang="fr">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Admin</title>
  <meta name="description" content="Plateforme d'amélioration de la maîtrise de la langue française">
  <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>
  <h3>Stats</h3>
  <table>
      <thead>
        <tr>
          <th>Module</th>
          <th>Complete</th>
        </tr>
      </thead>
      <tbody>
        {{~ it.stats.modules :module }}
          <tr>
            <td>{{= module.id }}</td>
            <td>{{? module.completedSteps !== module.stepCount }}no{{??}}yes{{?}}</td>
          </tr>
        {{~}}
      </tbody>
    </table>
  <h3>Stats (detail)</h3>
  <table>
    <thead>
      <tr>
        <th>Module</th>
        <th>Step</th>
        <th>Complete</th>
        <th>Score</th>
      </tr>
    </thead>
    <tbody>
      {{~ it.stats.modules :module }}
        {{~ module.steps :step }}
          <tr>
            <td>{{= module.id }}</td>
            <td>{{= step.id }}</td>
            <td>{{? step.complete === false }}no{{??}}yes{{?}}</td>
            <td>{{? step.score !== null }}{{= step.score }}{{??}}-{{?}}</td>
          </tr>
        {{~}}
      {{~}}
    </tbody>
  </table>
  <h3>Traces</h3>
  <table>
    <thead>
      <tr>
        <th>Module</th>
        <th>Step</th>
        <th>Type</th>
        <th>Complete</th>
        <th>Score</th>
        <th>Time</th>
      </tr>
    </thead>
    <tbody>
      {{~ it.traces :value:index }}
      <tr>
        <td>{{= value.module }}</td>
        <td>{{= value.step }}</td>
        <td>{{= value.type }}</td>
        <td>{{? value.complete === false }}no{{??}}yes{{?}}</td>
        <td>{{? value.score !== null }}{{= value.score }}{{??}}-{{?}}</td>
        <td>{{= value.time }}</td>
      </tr>
      {{~}}
    </tbody>
  </table>
</body>
</html>
