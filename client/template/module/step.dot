<h4>{{= it.moduleTitle }} ({{= it.currentStep }}/{{= it.stepCount }})</h4>
<div class="step-content"></div>

{{? it.previousStep }}
  <a href="#modules/{{= it.moduleId }}/steps/{{= it.previousStep }}">Previous</a>
{{?}}

{{? it.nextStep }}
  <a href="#modules/{{= it.moduleId }}/steps/{{= it.nextStep }}">Next</a>
{{?}}